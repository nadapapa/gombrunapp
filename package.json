{
    "name": "gombrunapp",
    "version": "0.0.0",
    "license": "MIT",
    "angular-cli": {},
    "scripts": {
        "start": "ng serve",
        "lint": "tslint \"src/**/*.ts\"",
        "test": "ng test -cc -sr",
        "test.watch": "ng test -cc -w",
        "pree2e": "webdriver-manager update",
        "e2e": "protractor",
        "start.ios": "cd nativescript && tns emulate ios",
        "start.livesync.ios": "cd nativescript && tns livesync ios --emulator --watch",
        "start.android.emulate": "cd nativescript && tns emulate android",
        "start.android.device": "cd nativescript && npm run android",
        "start.livesync.android": "cd nativescript && tns livesync android --emulator --watch",
        "build-electron": "ng build && cp src/electron/* dist/",
        "electron": "npm run build-electron && electron dist/",
        "start.android": "cd nativescript && tns emulate android"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^2.2.1",
        "@angular/compiler": "^2.2.1",
        "@angular/core": "^2.2.1",
        "@angular/forms": "^2.2.1",
        "@angular/http": "^2.2.1",
        "@angular/platform-browser": "^2.2.1",
        "@angular/platform-browser-dynamic": "^2.2.1",
        "@angular/router": "^3.2.1",
        "core-js": "^2.4.1",
        "electron": "^1.4.14",
        "enhanced-resolve": "^3.0.0",
        "rxjs": "^5.0.0",
        "ts-helpers": "^1.1.1",
        "zone.js": "^0.7.0"
    },
    "devDependencies": {
        "@angular/compiler-cli": "^2.2.1",
        "@types/jasmine": "^2.5.38",
        "@types/node": "^7.0.0",
        "angular-cli": "1.0.0-beta.28.3",
        "codelyzer": "~1.0.0-beta.3",
        "jasmine-core": "^2.5.2",
        "jasmine-spec-reporter": "^3.1.0",
        "karma": "^1.2.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-cli": "^1.0.1",
        "karma-jasmine": "^1.0.2",
        "karma-remap-istanbul": "^0.5.0",
        "protractor": "^5.0.0",
        "ts-node": "^2.0.0",
        "tslint": "^4.0.0",
        "typescript": "^2.0.0",
        "webdriver-manager": "^12.0.0"
    }
}
